<h2>Search Forex Signals</h2>
<form action="/forexsignals" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% forexsignals.forEach(forexsignals => { %>
    <div><%= forexsignals.name %></div>
<% }) %>